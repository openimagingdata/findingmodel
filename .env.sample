OPENAI_API_KEY=
# OPENAI_DEFAULT_MODEL=o1-mini
PERPLEXITY_API_KEY=
# PERPLEXITY_BASE_URL=https://api.perplexity.ai
# PERPLEXITY_DEFAULT_MODEL=sonar-pro

# Optional: Override manifest.json database URLs
# By default, database URLs/hashes are fetched from manifest.json
# Only set these if you need to use a different database source
# REMOTE_ANATOMIC_DB_URL=https://custom.host/anatomic_locations.duckdb
# REMOTE_ANATOMIC_DB_HASH=sha256:abc123...
# REMOTE_INDEX_DB_URL=https://custom.host/finding_models.duckdb
# REMOTE_INDEX_DB_HASH=sha256:def456...

# Optional: Override default database filenames (advanced usage)
# DUCKDB_ANATOMIC_PATH=anatomic_locations.duckdb
# DUCKDB_INDEX_PATH=finding_models.duckdb

# Optional: Logfire observability (for eval tracing and debugging)
# Get token from https://logfire.pydantic.dev/ after creating an account
# LOGFIRE_TOKEN=
# DISABLE_SEND_TO_LOGFIRE=false
# LOGFIRE_VERBOSE=false